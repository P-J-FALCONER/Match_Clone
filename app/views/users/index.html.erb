<h1>Index</h1>
future search bar
<div class="row">
<% @users.each do |user| %>
  <div class="col-sm-3 col-md-3">
    <div class="thumbnail">
      <div class="caption">
        <h3><%= user.first_name %> <%= user.last_name %></h3>
        <%= form_for @match_room do |f| %>
          <%= f.hidden_field :receiver_id, value: user.id %>
          <%= f.submit 'Get a room' %>
        <% end %>
      </div>
    </div>
  </div>
<% end %>
</div>
